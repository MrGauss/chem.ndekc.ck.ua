<div class="line" data-id="{tag:id}" data-inc_date="{tag:inc_date_unix}" data-lifetime="{tag:lifetime}" data-quantity_inc="{tag:quantity_inc}" data-quantity_left="{tag:quantity_left}" data-not_used_perc="{tag:not_used_perc}">
    <table>
        <tr>
            <td class="reagent">
                <span class="name">{tag:reagent:name}</span>
                <span class="quantity_left">Залишилось: {tag:quantity_left} {tag:reagent_units_short}</span>
            </td>
            <td class="number">[{tag:reagent_number}]</td>
            <td class="dead_date"><span class="dead_date"><label>Зіпсується:</label>{tag:dead_date}</span></td>
        </tr>
    </table>
</div>