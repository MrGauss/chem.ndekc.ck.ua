<div class="line" data-hash="{tag:hash}" data-stock_id="{tag:stock_id}" data-key="{tag:key}">
    <table>
        <tr>
            <td class="act" title="Дата та номер акту про перевірку">
                <span class="act_date">{tag:act_date}</span>
                <span class="act_number">{tag:act_number}</span>
            </td>
            <td class="expert" title="Перевірку здійснив">
                <span class="expert_surname">{tag:expert_surname}</span>
                <span class="expert_name">{tag:expert_name}</span>
                <span class="expert_phname">{tag:expert_phname}</span>
            </td>
            <td class="date_prolong">
                <span class="date_before_prolong" title="Термін придатності до перевірки">{tag:date_before_prolong}</span>
                <span class="date_after_prolong" title="Термін придатності після перевірки">{tag:date_after_prolong}</span>
            </td>
            <td class="remove"><span class="remove" data-role="remove"></span></td>
        </tr>
    </table>
</div>