<div class="line" data-id="{tag:id}" data-inc_date="{tag:inc_date_unix}">
    <table>
        <tr>
            <td class="numi">{tag:numi}</td>
            <td class="reagent">{tag:reagent_name}</td>
            <td class="inc_date">{tag:inc_date}</td>
            <td class="quantity_inc">{tag:quantity_inc}  {tag:reagent_units:1}</td>
            <td class="quantity_left">{tag:quantity_left} {tag:reagent_units:1}</td>
            <td class="quantity_dispersed">{tag:quantity_dispersed} {tag:reagent_units:1}</td>
            <td class="quantity_not_used">{tag:quantity_used} {tag:reagent_units:1}</td>
            <td></td>
        </tr>
    </table>
</div>