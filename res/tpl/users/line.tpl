<div class="line_user {access:label}" data-label="{access:label}" data-key="{tag:key}" data-id="{tag:id}" data-token="{RAND}">
    <div class="username">
        <span>{tag:surname}</span>
        <span>{tag:name}</span>
        <span>{tag:phname}</span>
    </div>
    <div class="access"><span>{access:name}</span></div>
    <div class="login">
        <span>{tag:login}</span>
    </div>
    <div class="lastip"><span>{tag:last_ip}</span></div>
    <div class="lastlogin"><span>{tag:ts}</span></div>

</div>